<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="netos.uc">
		<property name="javaFileEncoding" value="UTF-8" />
		<!-- 格式化java代码 -->
		<property name="javaFormatter"
			value="org.mybatis.generator.api.dom.DefaultJavaFormatter" />
		<!-- 格式化XML代码 -->
		<property name="xmlFormatter"
			value="org.mybatis.generator.api.dom.DefaultXmlFormatter" />

		<jdbcConnection
			connectionURL="jdbc:mysql://localhost:3306/netos.uc2?useSSL=false"
			driverClass="com.mysql.jdbc.Driver" password="mysql123" userId="cj" />

		<!-- java类型处理器 用于处理DB中的类型到Java中的类型，默认使用JavaTypeResolverDefaultImpl； 注意一点，默认会先尝试使用Integer，Long，Short等来对应DECIMAL和 
			NUMERIC数据类型； -->
		<javaTypeResolver
			type="org.mybatis.generator.internal.types.JavaTypeResolverDefaultImpl">
			<!-- true：使用BigDecimal对应DECIMAL和 NUMERIC数据类型 false：默认, scale>0;length>18：使用BigDecimal; 
				scale=0;length[10,18]：使用Long； scale=0;length[5,9]：使用Integer； scale=0;length<5：使用Short； -->
			<property name="forceBigDecimals" value="true" />
		</javaTypeResolver>

		<javaModelGenerator
			targetPackage="cj.netos.uc.domain"
			targetProject="cj.netos.uc.openport" />
		<sqlMapGenerator targetPackage="cj.db.mappings"
			targetProject="cj.netos.uc.plugin.core" />
		<javaClientGenerator
			targetPackage="cj.netos.uc.plugin.dao"
			targetProject="cj.netos.uc.plugin.core" type="XMLMAPPER" />
		<table schema="netos.uc2" domainObjectName="UcUser"
			tableName="uc_user" />
		<table schema="netos.uc2" domainObjectName="UcUserSegment"
			tableName="uc_user_seg" />
		<table schema="netos.uc2" domainObjectName="UcUserAttr"
			tableName="uc_user_attr" />
		<table schema="netos.uc2" domainObjectName="UcUserAttrVal"
			tableName="uc_user_attr_val" />
		<table schema="netos.uc2" domainObjectName="UcCompany"
			tableName="uc_company" />
		<table schema="netos.uc2" domainObjectName="UcDept"
			tableName="uc_dept" />
		<table schema="netos.uc2" domainObjectName="UcTenant"
			tableName="uc_tenant" />
		<table schema="netos.uc2" domainObjectName="TenantRole"
			tableName="tenant_role" />
		<table schema="netos.uc2" domainObjectName="TenantApp"
			tableName="tenant_app" />
		<table schema="netos.uc2" domainObjectName="TenantAccount"
			tableName="tenant_account" />
		<table schema="netos.uc2" domainObjectName="AppRole"
			tableName="app_role" />
		<table schema="netos.uc2" domainObjectName="UaAppRoleUser"
			tableName="ua_app_role_user" />
		<table schema="netos.uc2" domainObjectName="UcEmployee"
			tableName="uc_empl" />
		<table schema="netos.uc2" domainObjectName="UaRoleEmployee"
			tableName="ua_role_empl" />
		<table schema="netos.uc2" domainObjectName="UaRoleUser"
			tableName="ua_role_user" />
		<table schema="netos.uc2" domainObjectName="UaTenantRoleUser"
			tableName="ua_tenant_role_user" />
		<table schema="netos.uc2" domainObjectName="UcRole"
			tableName="uc_role" />
	</context>
</generatorConfiguration>